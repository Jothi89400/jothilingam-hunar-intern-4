import java.util.Scanner;

class LinkedList {
    static class Node {
        int data;
        Node next;

        Node(int data) {
            this.data = data;
            next = null;
        }
    }

    Node head;

    public void insert(int data) {
        Node newNode = new Node(data);
        if (head == null) {
            head = newNode;
            return;
        }
        Node temp = head;
        while (temp.next != null) {
            temp = temp.next;
        }
        temp.next = newNode;
    }

    public void deleteLastOccurrence(int key) {
        Node temp = head;
        Node prev = null;
        Node last = null;
        Node lastPrev = null;

        while (temp != null) {
            if (temp.data == key) {
                lastPrev = prev;
                last = temp;
            }
            prev = temp;
            temp = temp.next;
        }

        if (last == null) {
            System.out.println("Key not found in the list.");
            return;
        }

        if (last == head) {
            head = head.next;
        } else {
            lastPrev.next = last.next;
        }
    }

    public void display() {
        Node temp = head;
        while (temp != null) {
            System.out.print(temp.data + " ");
            temp = temp.next;
        }
        System.out.println();
    }

    public static void main(String[] args) {
        LinkedList list = new LinkedList();
        Scanner sc = new Scanner(System.in);

        
        System.out.println("Enter the number of elements you want to insert:");
        int n = sc.nextInt();
        
        System.out.println("Enter the elements:");
        for (int i = 0; i < n; i++) {
            list.insert(sc.nextInt());
        }

        System.out.println("Original list:");
        list.display();

        
        System.out.println("Enter the element whose last occurrence you want to delete:");
        int key = sc.nextInt();

        
        list.deleteLastOccurrence(key);

        System.out.println("After deleting last occurrence of " + key + ":");
        list.display();
        
        sc.close();
    }
}
